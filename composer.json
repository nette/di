{
	"name": "nette/di",
	"description": "ðŸ’Ž Nette Dependency Injection Container: Flexible, compiled and full-featured DIC with perfectly usable autowiring and support for all new PHP features.",
	"keywords": ["nette", "di", "dic", "ioc", "factory", "compiled", "static"],
	"homepage": "https://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"require": {
		"php": "8.1 - 8.4",
		"ext-tokenizer": "*",
		"ext-ctype": "*",
		"nette/neon": "^3.4",
		"nette/php-generator": "^4.1.6",
		"nette/robot-loader": "^4.0",
		"nette/schema": "^1.3",
		"nette/utils": "^4.0"
	},
	"require-dev": {
		"nette/tester": "^2.5.2",
		"tracy/tracy": "^2.9",
		"phpstan/phpstan-nette": "^2.0@stable"
	},
	"autoload": {
		"classmap": ["src/"],
		"psr-4": {
			"Nette\\": "src"
		}
	},
	"minimum-stability": "dev",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.0-dev"
		}
	}
}
